{
  "name": "esoja-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  },
  "scripts": {
    "start:local": "NODE_ENV=local nest start --debug --watch",
    "start:homolog": "NODE_ENV=homolog nest start --debug --watch",
    "start:prod": "node dist/main",
    "heroku-prebuild": "rimraf dist && npx prisma migrate deploy && npx prisma generate",
    "build": "nest build",
    "format": "prettier --write --loglevel=error \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:e2e": "NODE_ENV=test jest --testRegex='.*\\e2e.spec\\.ts$' --forceExit",
    "test.push:e2e": "NODE_ENV=test jest --testRegex='.*\\e2e.spec\\.ts$' --silent --noStackTrace --passWithNoTests --forceExit",
    "gitignore": "git update-index --assume-unchanged .vscode/thunder-tests/thunderEnvironment.db"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "^8.4.2",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/throttler": "^2.0.1",
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.1",
    "cache-manager": "^3.6.1",
    "cache-manager-redis-store": "^2.0.0",
    "cep-promise": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "git-last-commit": "^1.0.1",
    "luxon": "^2.4.0",
    "nestjs-prisma-querybuilder": "^0.0.12",
    "nodemailer": "^6.7.3",
    "passport-jwt": "^4.0.0",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "prisma": "^3.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run format && npm run lint"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
